<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16"
      fx:controller="com.example.quizmanagement.QuizController">
	<VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
		<children>
			
			<ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
				<image>
					<Image url="@../../../img/quiz.png"/>
				</image>
			</ImageView>
		</children>
	</VBox>
	<VBox prefHeight="400.0" prefWidth="100.0" fx:id="quizBox">
		<VBox.margin>
			<Insets left="30.0" right="30.0" top="5.0" bottom="5.0"/>
		</VBox.margin>
		<children>
			
			<Label fx:id="question"/>
		</children>
	</VBox>
	<HBox alignment="BOTTOM_CENTER" prefHeight="100.0" prefWidth="200.0">
		<children>
			<Button fx:id="goBack" mnemonicParsing="false" onAction="#onClickGoBack" text="Go back"/>
			<Button fx:id="click" mnemonicParsing="false" onAction="#onClickStart" text="Start"/>
		</children>
	</HBox>
	<Pane prefHeight="200.0" prefWidth="200.0"/>
</VBox>
