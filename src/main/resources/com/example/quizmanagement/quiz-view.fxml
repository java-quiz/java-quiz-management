<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" prefHeight="577.0" prefWidth="403.0"
      xmlns="http://javafx.com/javafx/11.0.14-internal" fx:controller="com.example.quizmanagement.QuizController">
	<VBox fx:id="quizBox" alignment="TOP_CENTER" prefHeight="250.0">
		<Label fx:id="question" textAlignment="CENTER">
			<font>
				<Font name="System Bold" size="20.0"/>
			</font>
			<VBox.margin>
				<Insets bottom="30.0" top="20.0"/>
			</VBox.margin>
		</Label>
		<GridPane fx:id="opBox" hgap="5.0" vgap="20.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0"/>
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" percentWidth="50.0"/>
			</columnConstraints>
			<rowConstraints>
				<RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" vgrow="SOMETIMES"/>
				<RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" vgrow="SOMETIMES"/>
			</rowConstraints>
			<VBox.margin>
				<Insets left="50.0" right="50.0"/>
			</VBox.margin>
		</GridPane>
		<opaqueInsets>
			<Insets/>
		</opaqueInsets>
		<VBox.margin>
			<Insets top="90.0"/>
		</VBox.margin>
	</VBox>
	<VBox alignment="CENTER">
		<VBox.margin>
			<Insets bottom="20.0" left="30" right="30"/>
		</VBox.margin>
		<HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
			<children>
				<Label fx:id="corLab" textFill="#088008">
					<font>
						<Font name="System Bold" size="15.0"/>
					</font>
					<HBox.margin>
						<Insets right="5.0"/>
					</HBox.margin>
				</Label>
				<Label fx:id="wrnLab" textFill="RED">
					<font>
						<Font name="System Bold" size="15.0"/>
					</font>
					<HBox.margin>
						<Insets left="5.0"/>
					</HBox.margin>
				</Label>
			</children>
		</HBox>
		<Label fx:id="report" style="-fx-text-fill: red;"/>
	</VBox>
	<HBox fx:id="btnContainer" alignment="BOTTOM_CENTER">
		<Button id="blue-button" fx:id="goBack" mnemonicParsing="false" onAction="#onClickGoBack" text="Exit">
			<HBox.margin>
				<Insets left="10.0" right="10.0"/>
			</HBox.margin>
		</Button>
		<Button id="purple-button" fx:id="nextButton" mnemonicParsing="false" onAction="#onClickStart" text="Next">
			<HBox.margin>
				<Insets left="10.0" right="10.0"/>
			</HBox.margin>
		</Button>
		<VBox.margin>
			<Insets bottom="30.0" top="30.0"/>
		</VBox.margin>
	</HBox>
	<VBox alignment="BOTTOM_CENTER">
		<children>
			<ProgressBar fx:id="pb" prefHeight="10.0" prefWidth="200.0" progress="0.0" style="-fx-accent: #6c63ff;"/>
		</children>
		<VBox.margin>
			<Insets/>
		</VBox.margin>
	</VBox>
</VBox>
