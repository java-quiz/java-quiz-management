<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="548.0" prefWidth="325.0"
      xmlns="http://javafx.com/javafx/11.0.14-internal" fx:controller="com.example.quizmanagement.ManageController">
	<children>
		<VBox alignment="TOP_CENTER">
			<children>
				<VBox fx:id="tableCon" alignment="TOP_CENTER">
					<VBox.margin>
						<Insets left="5.0" right="5.0" top="10.0"/>
					</VBox.margin>
				</VBox>
			</children>
			<VBox.margin>
				<Insets left="5.0" right="5.0"/>
			</VBox.margin>
		</VBox>
		<HBox alignment="BOTTOM_CENTER" prefHeight="45.0" prefWidth="200.0">
			<children>
				<Button id="blue-button" mnemonicParsing="false" onAction="#onExit" text="Exit">
					<HBox.margin>
						<Insets right="10.0"/>
					</HBox.margin>
				</Button>
				<Button id="red-button" mnemonicParsing="false" onAction="#deleteQs" text="Delete all question"/>
				<Button id="purple-button" fx:id="addButton" mnemonicParsing="false" onAction="#onAdd" text="Add question">
					<HBox.margin>
						<Insets left="10.0"/>
					</HBox.margin>
				</Button>
			</children>
		</HBox>
	</children>
</VBox>
